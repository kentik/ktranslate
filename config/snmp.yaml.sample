devices:
trap:
  listen: 127.0.0.1:162
  community: hello
  version: ""
  transport: ""
discovery:
  cidrs:
  - 10.10.0.0/24
  debug: false
  ports:
  - 161
  default_communities:
  - public
  default_v3: null
  add_devices: true
  threads: 4
  replace_devices: true
  use_snmp_v1: false
global:
  poll_time_sec: 300
  drop_if_outside_poll: false
  mib_profile_dir: profiles
  pymib_profile_dir: ""
  mibs_db: ""
  mibs_enabled:
  - IF-MIB
  timeout_ms: 3000
  retries: 0
